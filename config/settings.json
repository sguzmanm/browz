{
  "baseUrl": "http://localhost:8080/",
  "container": {
    "name": "sguzmanm/linux_cypress_tests:lite",
    "httpAppDir": "/tmp/app",
    "snapshotDestinationDir": "/tmp/runs",
    "configDir": "/tmp/config"
  },
  "visualizerServerConfig": {
    "port": "8080"
  },
  "httpServerConfig": {
    "port": "8080"
  },
  "snapshotProcessorServerConfig": {
    "port": "8081"
  },
  "baseBrowser": "chrome",
  "browsers": [
    "chrome",
    "firefox"
  ],
  "repository": {
    "branch": "master"
  },
  "browserWaitingResponseTime": "15000",
